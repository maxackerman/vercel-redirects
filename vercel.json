{
  "headers": [
    {
      "source": "/(.html)",
      "headers" : [
        {
          "key" : "X-Frame-Options",
          "value" : "DENY"
        },
        {
          "key" : "Content-Type",
          "value" : "text/html; charset=UTF-8"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/404" }
  ]
}
